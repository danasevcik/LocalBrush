<body id="newroom">
<div class="ui top attached button" tabindex="0"><%= link_to("LOGOUT", homepage_path, :method => 'get', class: 'css_class', style: 'color:#e22424') %></div>
<%# <%= button_to "LOGOUT", homepage_path, :method => 'get'%>

<h1><%= @user.name.downcase %>, add a new room!</h1>

<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <h4><%= error %></h4>
  <% end %>
<% end %>
<div id="projectcard">

<%= form_for @room do |f| %>
  <%= f.label "area (total square footage)" %><br>
  <%= f.number_field :area %><br>
  <%= f.file_field :image %>
  <!-- <form action="">
   <input type="file" name="image" accept="image/*">
   <input type="submit">
  </form> -->
  <%= f.submit %>
<% end %>
</div>

</body>
